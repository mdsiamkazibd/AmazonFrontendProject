{
  "version": 2,
  "builds": [
    {
      "src": "amazon.html",
      "use": "@vercel/static"
    },
    {
      "src": "checkout.html",
      "use": "@vercel/static"
    },
    {
      "src": "script/**",
      "use": "@vercel/static"
    },
    {
      "src": "style/**",
      "use": "@vercel/static"
    },
    {
      "src": "images/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/amazon.html"
    }
  ]
}
